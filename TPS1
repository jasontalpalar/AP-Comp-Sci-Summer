package tps1;

/**
 *
 * @author Jason
 */
public class TPS1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int array1[] = new int[10];
        int array2[] = new int[array1.length * 4];
        System.arraycopy(array1,0,array2,0,array1.length);
        array1 = array2;
    }
    
}
