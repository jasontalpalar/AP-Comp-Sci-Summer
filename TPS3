package tps3;

/**
 *
 * @author Jason
 */
public class TPS3 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int array1[][] = {{1, 2, 3, 4}, {5, 6, 7, 8}};
        for(int[] array2: array1) {
            for(int val: array2) {
                System.out.print(val);
            }
        }

    }
    
}
